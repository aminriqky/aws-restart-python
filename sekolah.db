-- Membuat tabel siswa

CREATE TABLE siswa (

  	id INTEGER PRIMARY KEy AUTOINCREMENT,

    nama varchar(50),

  	kelas varchar(10),

  	umur INTEGER

  )

-- Menampilkan data di tabel

SELECT * FROM Siswa
 
-- Menampilkan hanya beberapa kolom 

SELECT nama, kelas FROM Siswa
 
